{
    "urn": "urn:mediator:OMRStoDHIS2",
    "version": "1.0.0",
    "name": "OpenMRS to Dhis2 server",
    "description": "A mediator to post OpenMRS forms to Dhis2 server",
    "defaultChannelConfig": [
      {
        "name": "OpenMRS to Dhis2 server channel",
        "urlPattern": "^/reports$",
        "routes": [
          {
            "name": "OpenMRS to Dhis2 server Route",
            "host": "127.0.0.1",
            "path": "/reports",
            "port": "3000",
            "primary": true,
            "type": "http"
          }
        ],
        "allow": ["admin"],
        "methods": ["GET", "POST"],
        "type": "http"
      }
    ],
    "endpoints": [
      {
        "name": "Mediator Endpoint",
        "host": "127.0.0.1",
        "path": "/reports",
        "port": "3000",
        "primary": true,
        "type": "http"
      }
    ]
  }